[
  {
    "Action": [
      "sagemaker:InvokeEndpoint"
    ],
    "Resource": [
      "arn:aws:sagemaker:*:*:endpoint/*"
    ]
  },
  {
    "Action": [
      "s3:GetObject",
      "s3:PutObject",
      "s3:ListBucket",
      "s3:DeleteObject",
      "s3:ListObjects"
    ],
    "Resource": [
      "arn:aws:s3:::*",
      "arn:aws:s3:::*/*"
    ]
  }
]
